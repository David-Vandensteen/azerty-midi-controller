{
  "name": "azerty-midi-controller",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "serve": "nodemon --watch src -e js src/index.js",
    "master": "node src/index.js -m master",
    "slave": "node src/index.js -m slave",
    "test:slave": "node src/index.js -m slave -h 127.0.0.1 -p 7070 -k src/app/mapping/default.yaml",
    "test:master": "node src/index.js -m master -i mcc-in -o mcc-out -h 127.0.0.1 -p 7070",
    "lint": "eslint src"
  },
  "author": "David Vandensteen <dvandensteen@gmail.com>",
  "license": "MIT",
  "imports": {
    "#src/*": "./src/*.js"
  },
  "dependencies": {
    "app-root-path": "^3.1.0",
    "arg": "^5.0.2",
    "chalk": "^5.3.0",
    "custom-console-log": "github:David-Vandensteen/custom-console-log#release/1.0.0",
    "easymidi": "^3.1.0",
    "fs-extra": "^11.1.1",
    "midi-controller-store": "github:David-Vandensteen/midi-controller-store#release/1.0.0",
    "midi-normalizer": "github:David-Vandensteen/midi-normalizer#release/1.0.0",
    "net-keyboard": "git@github.com:David-Vandensteen/net-keyboard.git#release/1.0.0",
    "remote-midi": "git@github.com:David-Vandensteen/remote-midi.git#release/0.0.1",
    "yaml": "^2.3.2"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.28.1",
    "nodemon": "^3.0.1"
  }
}
